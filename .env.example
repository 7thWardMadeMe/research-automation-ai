# Copy this file to .env and fill in your API keys

# Toolhouse API Key - Get from https://app.toolhouse.ai/settings/api-keys
TOOLHOUSE_API_KEY=your_toolhouse_api_key_here

# OpenRouter API Key - Get from https://openrouter.ai
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: Your timezone offset (e.g., -8 for PST, +1 for CET)
TIMEZONE_OFFSET=-8

# Optional: Your name or identifier for personalized responses
USER_ID=your_name_here
